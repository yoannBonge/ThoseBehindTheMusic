{
  "version": 2,
  "builds": [
    { "src": "./index.cjs", "use": "@vercel/node" },
    { "src": "./api/**/*", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "https://those-behind-the-music.vercel.app"
      }
    }
  ]
}
